<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/negro"
    android:visibility="visible"
    tools:context=".MainActivity"
    tools:visibility="visible"
    tools:ignore="ExtraText">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="54dp"
        android:layout_height="26dp"
        android:id2="@+id/foto"
        android:src="@drawable/flechablanc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnbolsa_de_trabajo"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="750dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="54dp"
        android:layout_height="26dp"
        android:id2="@+id/foto"
        android:src="@drawable/flechablanc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnsoporte_tecnico"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="667dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="54dp"
        android:layout_height="26dp"
        android:id2="@+id/foto"
        android:src="@drawable/flechablanc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnnoticias"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="586dp" />

    <ImageView
        android:id="@+id/foto"
        android:layout_width="351dp"
        android:layout_height="276dp"
        android:src="@drawable/audi_mexico"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.156"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="54dp"
        android:layout_height="26dp"
        android:id2="@+id/foto"
        android:src="@drawable/flechablanc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnconocenos"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/btnnoticias"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_marginBottom="204dp"
        android:background="@color/negro"
        android:text="Noticias"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.668"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnconocenos"
        app:layout_constraintVertical_bias="0.403" />

    <Button
        android:id="@+id/btnbolsa_de_trabajo"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_marginBottom="72dp"
        android:background="@color/negro"
        android:text="   Bolsa de trabajo"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.603"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/btnconocenos"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:background="@color/negro"
        android:text="Conócenos"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.644"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/foto"
        app:layout_constraintVertical_bias="0.27" />

    <Button
        android:id="@+id/btnsoporte_tecnico"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:background="@color/negro"
        android:text="   Soporte técnico"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.613"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.808" />


</androidx.constraintlayout.widget.ConstraintLayout>